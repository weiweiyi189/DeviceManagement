<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="equipmentManagementSystem.Mybatis.ApprovalMapper">
    <select id="findAll" resultType="equipmentManagementSystem.entity.Approval">
        select *
        from approval
    </select>

    <delete id="deleteById" parameterType="Long">
        delete
        from approval
        where id=#{id}
    </delete>

    <select id="findById" resultType="equipmentManagementSystem.entity.Approval">
        select *
        from approval
        where id=#{id}
    </select>
</mapper>